{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "template-curly-spacing": [
      "error",
      "never"
    ]
  }
}
